<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_bottom_main"
    app:startDestination="@id/navigation_map">


    <fragment
        android:id="@+id/navigation_map"
        android:name="com.uogames.i_have_quest.ui.MapsFragment"
        android:label="MapsFragment"
        />

    <fragment
        android:id="@+id/navigation_person"
        android:name="com.uogames.i_have_quest.ui.PersonFragment"
        android:label="PersonFragment"
        tools:layout="@layout/fragment_person"/>

    <fragment
        android:id="@+id/navigation_backpack"
        android:name="com.uogames.i_have_quest.ui.BackpackFragment"
        android:label="BackpackFragment"
        tools:layout="@layout/fragment_backpack"
        />

    <fragment
        android:id="@+id/navigation_quest"
        android:name="com.uogames.i_have_quest.ui.QuestFragment"
        android:label="QuestFragment"
        tools:layout="@layout/fragment_quest"
        />
    <fragment
        android:id="@+id/navigation_chat"
        android:name="com.uogames.i_have_quest.ui.ChatMessagesFragment"
        android:label="ChatMessagesFragment"
        tools:layout="@layout/fragment_chat_messages"
        />


</navigation>